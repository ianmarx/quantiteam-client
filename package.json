{
  "name": "Quantiteam",
  "version": "0.1.0-beta",
  "description": "Front-end for quantiteam.com, written in React",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --inline --hot --port 7070 --mode development",
    "test": "jest test --coverage --maxWorkers=2",
    "build": "WEBPACK_ENV=production webpack --colors",
    "clean": "rimraf public/build",
    "surge:dev": "npm run build; surge -p public -d quantiteam-staging.surge.sh; npm run clean",
    "surge:prod": "npm run build; surge -p public -d quantiteam.com; npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianmarx/quantiteam-client.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/ianmarx/quantiteam-client/issues"
  },
  "homepage": "https://github.com/ianmarx/quantiteam-client#readme",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>test/setup.js"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jest": "^22.4.3",
    "jest-fetch-mock": "^1.5.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "supertest": "^3.0.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "jquery": "^3.3.1",
    "lodash.round": "^4.0.4",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-modal": "^3.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "victory": "^0.25.7"
  }
}
